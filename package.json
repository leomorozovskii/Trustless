    "@radix-ui/themes": "^3.0.5",
    "classnames": "^2.5.1",
    "i18next": "^23.11.5",
    "next": "14.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^13.5.0",
    "react-table": "^7.8.0",
    "view": "^1.1.1",
