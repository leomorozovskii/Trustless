{
  "name": "dapp",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:port": "next start -p ${PORT}",
    "lint": "next lint --quiet",
    "ts:check": "tsc --noEmit --project tsconfig.json",
    "fix": "npm run ts:check && npm run lint && npm run prettier-changed && npm run stylelint-changed",
    "prettier": "prettier --write pages components styles types mocks locales lib containers assets/icons e2e/src",
    "prettier-changed": "npx prettier --write $(git diff --name-only) $(git ls-files --others --exclude-standard)",
    "stylelint": "stylelint '**/*.scss' --fix",
    "stylelint-changed": "git diff --name-only | grep \"\\.scss$\" | xargs stylelint --fix"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@radix-ui/themes": "^3.0.5",
    "@rainbow-me/rainbowkit": "^2.1.1",
    "@tanstack/react-query": "^5.40.0",
    "@tanstack/react-table": "^8.17.3",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.11",
    "graphql": "^16.8.1",
    "graphql-request": "^7.0.1",
    "i18next": "^23.11.5",
    "next": "14.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^13.5.0",
    "react-tooltip": "^5.26.4",
    "vaul": "^0.9.1",
    "viem": "^2.13.1",
    "wagmi": "^2.9.7",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.43.1",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "3.3.0",
    "sass": "^1.77.2",
    "sass-loader": "^14.2.1",
    "stylelint": "^16.5.0",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.0",
    "stylelint-scss": "^6.3.0",
    "typescript": "^5.4.5",
    "typescript-plugin-css-modules": "^5.1.0"
  }
}